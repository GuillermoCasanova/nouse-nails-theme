@media only screen and (min-width: 940px) {
  .predictive-search__results-list
    .predictive-search__list-item:nth-child(2)
    ~ .predictive-search__list-item {
    display: flex;
  }

  .predictive-search__results-list
    .predictive-search__list-item:nth-child(4)
    ~ .predictive-search__list-item {
    display: none;
  }
}

.predictive-search__results-list
  .predictive-search__list-item:nth-child(2)
  ~ .predictive-search-option-search-keywords {
  display: flex;
}


.predictive-search__list-item {
  margin-bottom: var(--space-5);
}

@media screen and (min-width: 940px) {
  .predictive-search__list-item {
    margin-bottom: 0;
  }
}


.predictive-search-option-search-keywords {
  align-self: center;
  grid-column: 1 / 3;
  justify-content: center;
  justify-self: center;
  padding-top: var(--space-3);
  width: 100%;
}

@media only screen and (min-width: 940px) {
  .predictive-search-option-search-keywords {
    grid-column: 1 / 5;
    margin-bottom: var(--space-9);
  }
}


#predictive-search-results {
  padding: var(--space-4) 0 var(--space-6);
}


.predictive-search__cta {
  background-color: transparent;
  border: none;
  color: var(--color-base-text-1);
  display: block;
  font-family: var(--tertiary-font-family);
  font-size: var(--normal);
  line-height: 1.6;
  margin: var(--space-3) auto;
  max-width: 40rem;
  outline: 0;
  text-align: center;
  text-decoration: none;
}

.predictive-search__cta:hover,
.predictive-search__cta:focus {
  text-decoration: none;
}







.predictive-search__image-container {
  grid-area: product-image;
  margin-bottom: var(--space-2);
  overflow: hidden;
  position: relative;
  transition: all 0.3s var(--default-easing);
  width: 100%;
}

.predictive-search__item:hover .predictive-search__image-container,
.predictive-search__item:focus .predictive-search__image-container {
  transform: scale(1.02);
}

.predictive-search__image {
  object-fit: contain;
  padding-bottom: 140%;
  position: relative;
}

.predictive-search__image img {
  height: 100%;
  left: 0;
  object-fit: cover;
  position: absolute;
  right: 0;
  top: 0;
  width: 100%;
}
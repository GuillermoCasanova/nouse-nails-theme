{% style %}
    {{ settings.type_body_font | font_face: font_display: 'swap' }}
    {{ body_font_bold | font_face: font_display: 'swap' }}
    {{ body_font_italic | font_face: font_display: 'swap' }}
    {{ body_font_bold_italic | font_face: font_display: 'swap' }}
    {{ settings.type_header_font | font_face: font_display: 'swap' }}

    {% for scheme in settings.color_schemes -%}
    {% assign scheme_classes = scheme_classes | append: ', .color-' | append: scheme.id %}
    {% if forloop.index == 1 -%}
      :root,
    {%- endif %}
    .color-{{ scheme.id }} {
      /* Background Colors */
      --color-background: {{ scheme.settings.colors_background_1.red }}, {{ scheme.settings.colors_background_1.green }}, {{ scheme.settings.colors_background_1.blue }};
      --color-background-2: {{ scheme.settings.colors_background_2.red }}, {{ scheme.settings.colors_background_2.green }}, {{ scheme.settings.colors_background_2.blue }};
      --color-background-3: {{ scheme.settings.colors_background_3.red }}, {{ scheme.settings.colors_background_3.green }}, {{ scheme.settings.colors_background_3.blue }};

      /* Text Colors */
      --color-text: {{ scheme.settings.colors_text.red }}, {{ scheme.settings.colors_text.green }}, {{ scheme.settings.colors_text.blue }};
      --color-text-2: {{ scheme.settings.colors_text_2.red }}, {{ scheme.settings.colors_text_2.green }}, {{ scheme.settings.colors_text_2.blue }};

      /* Link Colors */
      --color-link: {{ scheme.settings.color_links.red }}, {{ scheme.settings.color_links.green }}, {{ scheme.settings.color_links.blue }};

      /* Button Colors */
      --color-button-primary-background: {{ scheme.settings.color_button_primary_background.red }}, {{ scheme.settings.color_button_primary_background.green }}, {{ scheme.settings.color_button_primary_background.blue }};
      --color-button-primary-text: {{ scheme.settings.color_button_primary_text.red }}, {{ scheme.settings.color_button_primary_text.green }}, {{ scheme.settings.color_button_primary_text.blue }};
      --color-button-primary-border: {{ scheme.settings.color_button_primary_border.red }}, {{ scheme.settings.color_button_primary_border.green }}, {{ scheme.settings.color_button_primary_border.blue }};
      --color-button-primary-hover-background: {{ scheme.settings.color_button_primary_hover_background.red }}, {{ scheme.settings.color_button_primary_hover_background.green }}, {{ scheme.settings.color_button_primary_hover_background.blue }};
      --color-button-primary-hover-text: {{ scheme.settings.color_button_primary_hover_text.red }}, {{ scheme.settings.color_button_primary_hover_text.green }}, {{ scheme.settings.color_button_primary_hover_text.blue }};
      --color-button-primary-hover-border: {{ scheme.settings.color_button_primary_hover_border.red }}, {{ scheme.settings.color_button_primary_hover_border.green }}, {{ scheme.settings.color_button_primary_hover_border.blue }};

      --color-button-secondary-background: {{ scheme.settings.color_button_secondary_background.red }}, {{ scheme.settings.color_button_secondary_background.green }}, {{ scheme.settings.color_button_secondary_background.blue }};
      --color-button-secondary-text: {{ scheme.settings.color_button_secondary_text.red }}, {{ scheme.settings.color_button_secondary_text.green }}, {{ scheme.settings.color_button_secondary_text.blue }};
      --color-button-secondary-border: {{ scheme.settings.color_button_secondary_border.red }}, {{ scheme.settings.color_button_secondary_border.green }}, {{ scheme.settings.color_button_secondary_border.blue }};
      --color-button-secondary-hover-background: {{ scheme.settings.color_button_secondary_hover_background.red }}, {{ scheme.settings.color_button_secondary_hover_background.green }}, {{ scheme.settings.color_button_secondary_hover_background.blue }};
      --color-button-secondary-hover-text: {{ scheme.settings.color_button_secondary_hover_text.red }}, {{ scheme.settings.color_button_secondary_hover_text.green }}, {{ scheme.settings.color_button_secondary_hover_text.blue }};
      --color-button-secondary-hover-border: {{ scheme.settings.color_button_secondary_hover_border.red }}, {{ scheme.settings.color_button_secondary_hover_border.green }}, {{ scheme.settings.color_button_secondary_hover_border.blue }};


      /* Accent Color */
      --color-accent: {{ scheme.settings.color_accent.red }}, {{ scheme.settings.color_accent.green }}, {{ scheme.settings.color_accent.blue }};

      /* Legacy compatibility variables */
      --color-foreground: {{ scheme.settings.colors_text.red }}, {{ scheme.settings.colors_text.green }}, {{ scheme.settings.colors_text.blue }};
      --color-button: {{ scheme.settings.color_button_primary_background.red }}, {{ scheme.settings.color_button_primary_background.green }}, {{ scheme.settings.color_button_primary_background.blue }};
      --color-button-text: {{ scheme.settings.color_button_primary_text.red }}, {{ scheme.settings.color_button_primary_text.green }}, {{ scheme.settings.color_button_primary_text.blue }};
      --color-secondary-button: {{ scheme.settings.color_button_secondary_background.red }}, {{ scheme.settings.color_button_secondary_background.green }}, {{ scheme.settings.color_button_secondary_background.blue }};
      --color-secondary-button-text: {{ scheme.settings.color_button_secondary_text.red }}, {{ scheme.settings.color_button_secondary_text.green }}, {{ scheme.settings.color_button_secondary_text.blue }};

      /* Shadow and badge colors (using text color for consistency) */
      --color-shadow: {{ scheme.settings.colors_text.red }}, {{ scheme.settings.colors_text.green }}, {{ scheme.settings.colors_text.blue }};
      --color-badge-foreground: {{ scheme.settings.colors_text.red }}, {{ scheme.settings.colors_text.green }}, {{ scheme.settings.colors_text.blue }};
      --color-badge-background: {{ scheme.settings.colors_background_1.red }}, {{ scheme.settings.colors_background_1.green }}, {{ scheme.settings.colors_background_1.blue }};
      --color-badge-border: {{ scheme.settings.colors_text.red }}, {{ scheme.settings.colors_text.green }}, {{ scheme.settings.colors_text.blue }};

      /* Payment terms background */
      --payment-terms-background-color: rgb({{ scheme.settings.colors_background_1.red }}, {{ scheme.settings.colors_background_1.green }}, {{ scheme.settings.colors_background_1.blue }});
    }
    {% endfor %}

    {{ scheme_classes | prepend: 'body' }} {
      color: rgba(var(--color-foreground), 1);
      background-color: rgb(var(--color-background));
    }

  :root {
    /* Base color variables for sections without color schemes */
    --color-brand-1: {{ settings.colors_brand_1.red }}, {{ settings.colors_brand_1.green }}, {{ settings.colors_brand_1.blue }};
    --color-brand-2: {{ settings.colors_brand_2.red }}, {{ settings.colors_brand_2.green }}, {{ settings.colors_brand_2.blue }};
    --color-base-accent-1: {{ settings.colors_accent_1.red }}, {{ settings.colors_accent_1.green }}, {{ settings.colors_accent_1.blue }};
    --color-base-text-1: {{ settings.colors_base_text_1.red }}, {{ settings.colors_base_text_1.green }}, {{ settings.colors_base_text_1.blue }};
    --color-base-text-2: {{ settings.colors_base_text_2.red }}, {{ settings.colors_base_text_2.green }}, {{ settings.colors_base_text_2.blue }};

    --color-error: 192, 76, 76;
    --default-easing: cubic-bezier(0.165, 0.84, 0.44, 1);
    --payment-terms-background-color: {{ settings.colors_background_1 }};

    --media-padding: {{ settings.media_padding }}px;
    --media-border-opacity: {{ settings.media_border_opacity | divided_by: 100.0 }};
    --media-border-width: {{ settings.media_border_thickness }}px;
    --media-radius: {{ settings.media_radius }}px;
    --media-shadow-opacity: {{ settings.media_shadow_opacity | divided_by: 100.0 }};
    --media-shadow-horizontal-offset: {{ settings.media_shadow_horizontal_offset }}px;
    --media-shadow-vertical-offset: {{ settings.media_shadow_vertical_offset }}px;
    --media-shadow-blur-radius: {{ settings.media_shadow_blur }}px;

    --page-width: {{ settings.page_width | divided_by: 10 }}rem;
    --page-width-margin: {% if settings.page_width == '1600' %}2{% else %}0{% endif %}rem;

    --card-image-padding: {{ settings.card_image_padding | divided_by: 10.0 }}rem;
    --card-corner-radius: {{ settings.card_corner_radius | divided_by: 10.0 }}rem;
    --card-text-alignment: {{ settings.card_text_alignment }};
    --card-border-width: {{ settings.card_border_thickness | divided_by: 10.0 }}rem;
    --card-border-opacity: {{ settings.card_border_opacity | divided_by: 100.0 }};
    --card-shadow-opacity: {{ settings.card_shadow_opacity | divided_by: 100.0 }};
    --card-shadow-horizontal-offset: {{ settings.card_shadow_horizontal_offset | divided_by: 10.0 }}rem;
    --card-shadow-vertical-offset: {{ settings.card_shadow_vertical_offset | divided_by: 10.0 }}rem;
    --card-shadow-blur-radius: {{ settings.card_shadow_blur | divided_by: 10.0 }}rem;

    --badge-corner-radius: {{ settings.badge_corner_radius | divided_by: 10.0 }}rem;

    --popup-border-width: {{ settings.popup_border_thickness }}px;
    --popup-border-opacity: {{ settings.popup_border_opacity | divided_by: 100.0 }};
    --popup-corner-radius: {{ settings.popup_corner_radius }}px;
    --popup-shadow-opacity: {{ settings.popup_shadow_opacity | divided_by: 100.0 }};
    --popup-shadow-horizontal-offset: {{ settings.popup_shadow_horizontal_offset }}px;
    --popup-shadow-vertical-offset: {{ settings.popup_shadow_vertical_offset }}px;
    --popup-shadow-blur-radius: {{ settings.popup_shadow_blur }}px;

    --drawer-border-width: {{ settings.drawer_border_thickness }}px;
    --drawer-border-opacity: {{ settings.drawer_border_opacity | divided_by: 100.0 }};
    --drawer-shadow-opacity: {{ settings.drawer_shadow_opacity | divided_by: 100.0 }};
    --drawer-shadow-horizontal-offset: {{ settings.drawer_shadow_horizontal_offset }}px;
    --drawer-shadow-vertical-offset: {{ settings.drawer_shadow_vertical_offset }}px;
    --drawer-shadow-blur-radius: {{ settings.drawer_shadow_blur }}px;

    --spacing-sections-desktop: {{ settings.spacing_sections }}px;
    --spacing-sections-mobile: {% if settings.spacing_sections < 24 %}{{ settings.spacing_sections }}{% else %}{{ settings.spacing_sections | times: 0.7 | round | at_least: 20 }}{% endif %}px;

    --grid-desktop-vertical-spacing: {{ settings.spacing_grid_vertical }}px;
    --grid-desktop-horizontal-spacing: {{ settings.spacing_grid_horizontal }}px;
    --grid-mobile-vertical-spacing: {{ settings.spacing_grid_vertical | divided_by: 2 }}px;
    --grid-mobile-horizontal-spacing: {{ settings.spacing_grid_horizontal | divided_by: 2 }}px;

    --text-boxes-border-opacity: {{ settings.text_boxes_border_opacity | divided_by: 100.0 }};
    --text-boxes-border-width: {{ settings.text_boxes_border_thickness }}px;
    --text-boxes-radius: {{ settings.text_boxes_radius }}px;
    --text-boxes-shadow-opacity: {{ settings.text_boxes_shadow_opacity | divided_by: 100.0 }};
    --text-boxes-shadow-horizontal-offset: {{ settings.text_boxes_shadow_horizontal_offset }}px;
    --text-boxes-shadow-vertical-offset: {{ settings.text_boxes_shadow_vertical_offset }}px;
    --text-boxes-shadow-blur-radius: {{ settings.text_boxes_shadow_blur }}px;

    --buttons-radius: {{ settings.buttons_radius }}px;
    --buttons-radius-outset: {% if settings.buttons_radius > 0 %}{{ settings.buttons_radius | plus: settings.buttons_border_thickness }}{% else %}0{% endif %}px;
    --buttons-border-width: {% if settings.buttons_border_opacity > 0 %}{{ settings.buttons_border_thickness }}{% else %}0{% endif %}px;
    --buttons-border-opacity: {{ settings.buttons_border_opacity | divided_by: 100.0 }};
    --buttons-shadow-opacity: {{ settings.buttons_shadow_opacity | divided_by: 100.0 }};
    --buttons-shadow-horizontal-offset: {{ settings.buttons_shadow_horizontal_offset }}px;
    --buttons-shadow-vertical-offset: {{ settings.buttons_shadow_vertical_offset }}px;
    --buttons-shadow-blur-radius: {{ settings.buttons_shadow_blur }}px;
    --buttons-border-offset: {% if settings.buttons_radius > 0 or settings.buttons_shadow_opacity > 0 %}0.3{% else %}0{% endif %}px;

    --inputs-radius: {{ settings.inputs_radius }}px;
    --inputs-border-width: {{ settings.inputs_border_thickness }}px;
    --inputs-border-opacity: {{ settings.inputs_border_opacity | divided_by: 100.0 }};
    --inputs-shadow-opacity: {{ settings.inputs_shadow_opacity | divided_by: 100.0 }};
    --inputs-shadow-horizontal-offset: {{ settings.inputs_shadow_horizontal_offset }}px;
    --inputs-margin-offset: {% if settings.inputs_shadow_vertical_offset != 0 and settings.inputs_shadow_opacity > 0 %}{{ settings.inputs_shadow_vertical_offset | abs }}{% else %}0{% endif %}px;
    --inputs-shadow-vertical-offset: {{ settings.inputs_shadow_vertical_offset }}px;
    --inputs-shadow-blur-radius: {{ settings.inputs_shadow_blur }}px;
    --inputs-radius-outset: {% if settings.inputs_radius > 0 %}{{ settings.inputs_radius | plus: settings.inputs_border_thickness }}{% else %}0{% endif %}px;

    --variant-pills-radius: {{ settings.variant_pills_radius }}px;
    --variant-pills-border-width: {{ settings.variant_pills_border_thickness }}px;
    --variant-pills-border-opacity: {{ settings.variant_pills_border_opacity | divided_by: 100.0 }};
    --variant-pills-shadow-opacity: {{ settings.variant_pills_shadow_opacity | divided_by: 100.0 }};
    --variant-pills-shadow-horizontal-offset: {{ settings.variant_pills_shadow_horizontal_offset }}px;
    --variant-pills-shadow-vertical-offset: {{ settings.variant_pills_shadow_vertical_offset }}px;
    --variant-pills-shadow-blur-radius: {{ settings.variant_pills_shadow_blur }}px;

    --primary-font-family: "Mabry-Medium", sans-serif;
    --secondary-font-family: "Mabry-Regular", serif;
    --tertiary-font-family: "Mabry-Bold", sans-serif;
    --font-weight-regular: 600;
    --font-weight-bold: 800;
    --letter-spacing-1: 1.5px;
    --letter-spacing-2: 1px;
    --letter-spacing-3: 3px;
    --letter-spacing-4: 5px;
    --line-height-normal: 1.25;
    --line-height-double: 1.5;
    --site--min: 15;
    --site--max: 100;
    --headline-font-family: var(--primary-font-family);
    --headline-line-height: 1.1;
    --headline-font-weight: 500;
    --text-font-family: var(--secondary-font-family);
    --text-letter-spacing: 0;
    --text-line-height: 1.25;
    --text-large-line-height: 1;


    /*Border Radius*/
    --border-radius-x-small: .25rem;
    --border-radius-small: .5rem;
    --border-radius-main: 1.25rem;
    --border-radius-large: 2rem;

    --space-0: 0;
    --space-1: .3125rem; /* 5px */
    --space-2: clamp(0.3125 * 1rem, ((0.3125 - ((0.625 - 0.3125) / (var(--site--max) - var(--site--min)) * var(--site--min))) * 1rem + ((0.625 - 0.3125) / (var(--site--max) - var(--site--min))) * 100vw), 0.625 * 1rem); /* 5px / 10px */
    --space-3: clamp(0.625 * 1rem, ((0.625 - ((0.9375 - 0.625) / (var(--site--max) - var(--site--min)) * var(--site--min))) * 1rem + ((0.9375 - 0.625) / (var(--site--max) - var(--site--min))) * 100vw), 0.9375 * 1rem); /* 10px / 15px */
    --space-4: clamp(0.9375 * 1rem, ((0.9375 - ((1.25 - 0.9375) / (var(--site--max) - var(--site--min)) * var(--site--min))) * 1rem + ((1.25 - 0.9375) / (var(--site--max) - var(--site--min))) * 100vw), 1.25 * 1rem); /* 15px / 20px*/
    --space-5: clamp(1.25 * 1rem, ((1.25 - ((1.5625 - 1.25) / (var(--site--max) - var(--site--min)) * var(--site--min))) * 1rem + ((1.5625 - 1.25) / (var(--site--max) - var(--site--min))) * 100vw), 1.5625 * 1rem); /* 20px / 25px */
    --space-6: clamp(1.40625 * 1rem, ((1.40625 - ((2.1875 - 1.40625) / (var(--site--max) - var(--site--min)) * var(--site--min))) * 1rem + ((2.1875 - 1.40625) / (var(--site--max) - var(--site--min))) * 100vw), 2.1875 * 1rem); /* 22.5px / 35px */
    --space-7: clamp(1.5625 * 1rem, ((1.5625 - ((2.5 - 1.5625) / (var(--site--max) - var(--site--min)) * var(--site--min))) * 1rem + ((2.5 - 1.5625) / (var(--site--max) - var(--site--min))) * 100vw), 2.5 * 1rem); /* 25px / 40px */
    --space-8: clamp(1.875 * 1rem, ((1.875 - ((2.8125 - 1.875) / (var(--site--max) - var(--site--min)) * var(--site--min))) * 1rem + ((2.8125 - 1.875) / (var(--site--max) - var(--site--min))) * 100vw), 2.8125 * 1rem); /* 30px / 45px */
    --space-9: clamp(1.875 * 1rem, ((1.875 - ((4.0625 - 1.875) / (var(--site--max) - var(--site--min)) * var(--site--min))) * 1rem + ((4.0625 - 1.875) / (var(--site--max) - var(--site--min))) * 100vw), 4.0625 * 1rem); /* 30px / 65px */
    --space-10: clamp(2.1875 * 1rem, ((2.1875 - ((5.625 - 2.1875) / (var(--site--max) - var(--site--min)) * var(--site--min))) * 1rem + ((5.625 - 2.1875) / (var(--site--max) - var(--site--min))) * 100vw), 5.625 * 1rem); /* 35px / 90px */



    /*--Section Padding--*/
    --section-padding-x-small: clamp(2.1875 * 1rem, ((2.1875 - ((3.125 - 2.1875) / (var(--site--max) - var(--site--min)) * var(--site--min))) * 1rem + ((3.125 - 2.1875) / (var(--site--max) - var(--site--min))) * 100vw), 3.125 * 1rem);
    --section-padding-small: clamp(2.8125 * 1rem, ((2.8125 - ((3.75 - 2.8125) / (var(--site--max) - var(--site--min)) * var(--site--min))) * 1rem + ((3.75 - 2.8125) / (var(--site--max) - var(--site--min))) * 100vw), 3.75 * 1rem); /* 45px / 60px */
    --section-padding-main: clamp(4 * 1rem, ((4 - ((7 - 4) / (var(--site--max) - var(--site--min)) * var(--site--min))) * 1rem + ((7 - 4) / (var(--site--max) - var(--site--min))) * 100vw), 7 * 1rem);
    --section-padding-large: clamp(5.5 * 1rem, ((5.5 - ((10 - 5.5) / (var(--site--max) - var(--site--min)) * var(--site--min))) * 1rem + ((10 - 5.5) / (var(--site--max) - var(--site--min))) * 100vw), 10 * 1rem);

    /*Grid Padding Radius*/
    --grid-padding-small: var(--space-6);
    --grid-padding-main: var(--space-6);
    --grid-padding-large: var(--space-9);

    /*--Card Padding--*/
    --padding-x-small: clamp(0.4 * 1rem, ((0.4 - ((0.75 - 0.4) / (var(--site--max) - var(--site--min)) * var(--site--min))) * 1rem + ((0.75 - 0.4) / (var(--site--max) - var(--site--min))) * 100vw), 0.75 * 1rem);
    --padding-small: clamp(0.6 * 1rem, ((0.6 - ((1 - 0.6) / (var(--site--max) - var(--site--min)) * var(--site--min))) * 1rem + ((1 - 0.6) / (var(--site--max) - var(--site--min))) * 100vw), 1 * 1rem);
    --padding-main: clamp(1.5625 * 1rem, ((1.5625 - ((2.8125 - 1.5625) / (var(--site--max) - var(--site--min)) * var(--site--min))) * 1rem + ((2.8125 - 1.5625) / (var(--site--max) - var(--site--min))) * 100vw), 2.8125 * 1rem); /* 25px / 45px */
    --padding-large: clamp(2.1875 * 1rem, ((2.1875 - ((3.125 - 2.1875) / (var(--site--max) - var(--site--min)) * var(--site--min))) * 1rem + ((3.125 - 2.1875) / (var(--site--max) - var(--site--min))) * 100vw), 3.125 * 1rem);



    /*Container sizes*/
    --container-padding: var(--grid-padding-main);
    --max-width-x-small: 75rem;
    --max-width-small: 92rem;
    --max-width-medium: calc(var(--site--max) * 1rem);;
    --max-width-large: 125rem;
    --max-width-xlarge: 130rem;

    /* Headline sizes */
    /*Website to get CLAMP functions: https://ms-clamp-function-builder.lovable.app/ */

    /*--Mobile: 45px (4.06rem) → Desktop: 90px (6.25rem)--*/
    --headline-size-display: clamp(2.8125 * 1rem, ((2.8125 - ((5.625 - 2.8125) / (var(--site--max) - var(--site--min)) * var(--site--min))) * 1rem + ((5.625 - 2.8125) / (var(--site--max) - var(--site--min))) * 100vw), 5.625 * 1rem);

    /*--Mobile: 45px (2.81rem) → Desktop: 55px (3.75rem)--*/
    --headline-size-h1: clamp(2.8125 * 1rem, ((2.8125 - ((3.4375 - 2.8125) / (var(--site--max) - var(--site--min)) * var(--site--min))) * 1rem + ((3.4375 - 2.8125) / (var(--site--max) - var(--site--min))) * 100vw), 3.4375 * 1rem);

    /*--Mobile: 35px (2.19rem) → Desktop: 40px (2.81rem)--*/
    --headline-size-h2: clamp(2.1875 * 1rem, ((2.1875 - ((2.5 - 2.1875) / (var(--site--max) - var(--site--min)) * var(--site--min))) * 1rem + ((2.5 - 2.1875) / (var(--site--max) - var(--site--min))) * 100vw), 2.5 * 1rem);

    /*--Mobile: 25px (1.56rem) → Desktop: 35px (2.19rem)--*/
    --headline-size-h3: clamp(1.5625 * 1rem, ((1.5625 - ((2.1875 - 1.5625) / (var(--site--max) - var(--site--min)) * var(--site--min))) * 1rem + ((2.1875 - 1.5625) / (var(--site--max) - var(--site--min))) * 100vw), 2.1875 * 1rem);

    /*--Mobile: 20px (1.25rem) → Desktop: 25px (1.56rem)--*/
    --headline-size-h4: clamp(1.25 * 1rem, ((1.25 - ((1.5625 - 1.25) / (var(--site--max) - var(--site--min)) * var(--site--min))) * 1rem + ((1.5625 - 1.25) / (var(--site--max) - var(--site--min))) * 100vw), 1.5625 * 1rem);

    /*--Mobile: 18px (1.13rem) → Desktop: 20px (1.25rem)--*/
    --headline-size-h5: clamp(1.125 * 1rem, ((1.125 - ((1.25 - 1.125) / (var(--site--max) - var(--site--min)) * var(--site--min))) * 1rem + ((1.25 - 1.125) / (var(--site--max) - var(--site--min))) * 100vw), 1.25 * 1rem);

    /*--Mobile: 16px (1.00rem) → Desktop: 18px (1.13rem)--*/
    --headline-size-h6:  clamp(1 * 1rem, ((1 - ((1.125 - 1) / (var(--site--max) - var(--site--min)) * var(--site--min))) * 1rem + ((1.125 - 1) / (var(--site--max) - var(--site--min))) * 100vw), 1.125 * 1rem);



    /* Text sizes */

    /*--Mobile: 14px (1.00rem) → Desktop: 14px (1.00rem)--*/
    --text-size-tiny: clamp(0.7 * 1rem, ((0.7 - ((0.875 - 0.7) / (var(--site--max) - var(--site--min)) * var(--site--min))) * 1rem + ((0.875 - 0.7) / (var(--site--max) - var(--site--min))) * 100vw), 0.875 * 1rem);

    /*--Mobile: 16px (1.00rem) → Desktop: 16px (1.00rem)--*/
    --text-size-small: clamp(0.875 * 1rem, ((0.875 - ((1 - 0.875) / (var(--site--max) - var(--site--min)) * var(--site--min))) * 1rem + ((1 - 0.875) / (var(--site--max) - var(--site--min))) * 100vw), 1 * 1rem);

    /*--Mobile: 18px (1.13rem) → Desktop: 18px (1.13rem)--*/
    --text-size-main: clamp(1.125 * 1rem, ((1.125 - ((1.125 - 1.125) / (var(--site--max) - var(--site--min)) * var(--site--min))) * 1rem + ((1.125 - 1.125) / (var(--site--max) - var(--site--min))) * 100vw), 1.125 * 1rem);

    /*--Mobile: 18px (1.13rem) → Desktop: 20px (1.25rem)--*/
    --text-size-medium: clamp(1.25 * 1rem, ((1.25 - ((1.25 - 1.25) / (var(--site--max) - var(--site--min)) * var(--site--min))) * 1rem + ((1.25 - 1.25) / (var(--site--max) - var(--site--min))) * 100vw), 1.25 * 1rem);

    /*--Mobile: 15px (1.25rem) → Desktop: 25px (1.56rem)--*/
    --text-size-large:  clamp(0.9375 * 1rem, ((0.9375 - ((1.5625 - 0.9375) / (var(--site--max) - var(--site--min)) * var(--site--min))) * 1rem + ((1.5625 - 0.9375) / (var(--site--max) - var(--site--min))) * 100vw), 1.5625 * 1rem);

    /*--Mobile: 20px (1.56rem) → Desktop: 30px (2.19rem)--*/
    --text-size-x-large: clamp(1.25 * 1rem, ((1.25 - ((1.875 - 1.25) / (var(--site--max) - var(--site--min)) * var(--site--min))) * 1rem + ((1.875 - 1.25) / (var(--site--max) - var(--site--min))) * 100vw), 1.875 * 1rem);

    /*--Mobile: 25px (1.56rem) → Desktop: 35px (2.19rem)--*/
    --text-size-xx-large:  clamp(1.5625 * 1rem, ((1.5625 - ((2.1875 - 1.5625) / (var(--site--max) - var(--site--min)) * var(--site--min))) * 1rem + ((2.1875 - 1.5625) / (var(--site--max) - var(--site--min))) * 100vw), 2.1875 * 1rem);

    /* Buttons */
    --button-font-weight: 500;
    --button-letter-spacing: .05rem;
    --button-line-height: 1;

    /*Easing*/
    --main-easing: cubic-bezier(0.5, 1, 0.89, 1);
  }


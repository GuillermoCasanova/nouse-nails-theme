    "hae_faqs_ACNzbj": {
      "type": "hae-faqs",
      "blocks": {
        "faq_block_jRAPtz": {
          "type": "faq_block",
          "settings": {
            "question": "How long do they last?",
            "answer": "<p>Up to 14 days of chip-free shine. For best results, use the prep pad, apply on dry nails, and avoid oils or baths for 1-2 hours after. They’re tough enough for everyday life—like a screen protector for your nails.</p>"
          }
        },
        "faq_block_43ptYe": {
          "type": "faq_block",
          "settings": {
            "question": "How do I apply?",
            "answer": "<p>So easy! Find your strip size, peel & stick, then cure for 60 seconds. Trim or buff any excess. Voilà, salon-level nails in 15 minutes. For a complete guide, <a href=\"/pages/how-to\" target=\"_blank\" title=\"How To\"><span style=\"text-decoration:underline\">click here</span></a>.</p>"
          }
        },
        "faq_block_ABp3fi": {
          "type": "faq_block",
          "settings": {
            "question": "Are they non-toxic?",
            "answer": "<p>No nasties, just nail magic. We skip the top 10 questionable ingredients—no HEMA, DBP, TPHP, toluene, or formaldehyde. Clean, safe, and worry-free.</p>"
          }
        },
        "faq_block_QL3tzy": {
          "type": "faq_block",
          "settings": {
            "question": "How do I remove them?",
            "answer": "<p>Gently lift with the provided wooden stick. For ideal results, use our <a href=\"/products/ctrl-delete-nail-remover-serum\" title=\"Ctrl+Delete Vitamin Serum Remover\">Ctrl+Delete Vitamin Remover Serum</a> for easy melt-off-the-nail removal. </p><p>No damage, just nourished nails underneath.</p>"
          }
        },
        "faq_block_N3AeaJ": {
          "type": "faq_block",
          "settings": {
            "question": "Do they really protect my nails?",
            "answer": "<p>Yes! They act like a screen protector for your nails—shielding against chips and breaks while helping weak or brittle nails grow stronger, all with zero damage on removal.</p>"
          }
        }
      },

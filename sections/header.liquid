      <div class="header__icons page-width">
        <search-toggle class="show-for-medium-up">
          <button
            class="header__icon"
            id="search-modal-toggle"
            data-search-modal-toggle
            aria-expanded="false"
            aria-haspopup="dialog"
            aria-controls="search-modal"
          >
            <span>
              <svg class="icon" aria-hidden="true" focusable="false" role="presentation">
                <use href="#icon-search"></use>
              </svg>
            </span>
          </button>
        </search-toggle>

        <div class="show-for-medium-up">
          <a href="/pages/swym-wishlist" class="header__icon link focus-inset">
            <span class="visually-hidden"> View Favorites </span>
            <span class="svg-wrapper">{{ 'icon-favorite.svg' | inline_asset_content }}</span>
          </a>
        </div>

        <div class="show-for-large-up">
          <a
            href="{%- if customer -%}{{ routes.account_url }}{%- else -%}{{ routes.account_login_url }}{%- endif -%}"
            class="header__icon link focus-inset"
          >
            <svg class="icon" aria-hidden="true" focusable="false" role="presentation">
              <use href="#icon-account"></use>
            </svg>
            <span class="visually-hidden">
              {%- liquid
                if customer
                  echo 'customer.account_fallback' | t
                else
                  echo 'customer.log_in' | t
                endif
              -%}
            </span>
          </a>
        </div>

        <a href="{{ routes.cart_url }}" class="header__icon link focus-inset" id="cart-icon-bubble" data-cart-toggle>
          <svg class="icon icon-cart" aria-hidden="true" focusable="false" role="presentation">
            <use href="#icon-bag"></use>
          </svg>
          <span data-cart-toggle-text class="visually-hidden  header__cart-button__text">
            {{- 'templates.cart.cart' | t -}}
          </span>
          {%- if cart != empty -%}
            <div class="cart-count-bubble">
              {%- if cart.item_count < 100 -%}
                <span aria-hidden="true">{{ cart.item_count }}</span>
              {%- endif -%}
              <span class="visually-hidden">{{ 'sections.header.cart_count' | t: count: cart.item_count }}</span>
            </div>
          {%- endif -%}
        </a>
      </div>
    </div>

    <div
      class="search-modal preload"
      data-search-modal
      aria-hidden="true"
      id="search-modal"
      role="dialog"
      aria-modal="true"
      aria-labelledby="search-modal-toggle"
      aria-label="{{ 'general.search.search' | t }}"
    >
      <div class="search-modal__content" tabindex="-1">
        <predictive-search class="search-modal__form" data-loading-text="{{ 'accessibility.loading' | t }}">
          <form action="{{ routes.search_url }}" method="get" role="search" class="search search-modal__form">
            <custom-input-wrapper>
              <div class="search__input-container page-width">
                <input
                  class="search__input"
                  id="Search-In-Modal-1"
                  type="search"
                  name="q"
                  value=""
                  role="combobox"
                  aria-expanded="false"
                  aria-owns="predictive-search-results-list"
                  aria-controls="predictive-search-results-list"
                  aria-haspopup="listbox"
                  aria-autocomplete="list"
                  autocorrect="off"
                  autocomplete="off"
                  autocapitalize="off"
                  spellcheck="false"
                >
                <label data-search-input-label class="search__label" for="Search-In-Modal-1">
                  {{ 'general.search.search' | t }}
                </label>
                <input type="hidden" name="options[prefix]" value="last">

                <button class="search__button" aria-label="{{ 'general.search.search' | t }}" data-search-modal-toggle>
                  <svg class="icon icon-search" aria-hidden="true" focusable="false" role="presentation">
                    <use href="#icon-search"></use>
                  </svg>
                </button>
                <button
                  class="search-modal__close button-close"
                  type="button"
                  aria-label="{{ 'accessibility.close' | t }}"
                  data-modal-close
                >
                  <svg class="icon icon-close" aria-hidden="true" focusable="false" role="presentation">
                    <use href="#icon-close"></use>
                  </svg>
                  <span class="visually-hidden">
                    {{ 'accessibility.close' | t }}
                  </span>
                </button>
              </div>
            </custom-input-wrapper>

            <div class="predictive-search predictive-search--header" tabindex="-1" data-predictive-search>
              <div class="predictive-search__loading-state">
                <svg
                  aria-hidden="true"
                  focusable="false"
                  role="presentation"
                  class="spinner"
                  viewBox="0 0 66 66"
                  xmlns="http://www.w3.org/2000/svg"
                >
                  <circle class="path" fill="none" stroke-width="6" cx="33" cy="33" r="30"></circle>
                </svg>
              </div>
            </div>
            <span class="predictive-search-status visually-hidden" role="status" aria-hidden="true"></span>
          </form>
        </predictive-search>
      </div>
    </div>
  </header>
</sticky-header>

